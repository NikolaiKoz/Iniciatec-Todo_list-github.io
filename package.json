{
  "name": "iniciatec-todo_list-github.io",
  "version": "1.0.0",
  "description": "This is a Todo list project using pug ,sass, typescript and firebase.",
  "main": "index.js",
  "scripts": {
    "watch:pug": "nodemon --watch src --ext pug build.js",
    "watch:sass": "nodemon --watch src/scss --ext scss --exec 'npx sass src/scss/index.scss:dist/css/app.css'",
    "watch:ts": "nodemon --watch src/ts --ext ts --exec 'tsc -p .'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikolaiKoz/Iniciatec-Todo_list-github.io.git"
  },
  "keywords": [
    "todo",
    "list"
  ],
  "author": "Vallejos Jose Luis",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NikolaiKoz/Iniciatec-Todo_list-github.io/issues"
  },
  "homepage": "https://github.com/NikolaiKoz/Iniciatec-Todo_list-github.io#readme",
  "dependencies": {
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  }
}
