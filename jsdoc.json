{
    "plugins": [],
    "source": {
      "include": ["./src/ts"],
      "includePattern": ".ts$",
      "excludePattern": "(node_modules|docs|test)"
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "recurse": true,
      "destination": "./jsdocs",
      "readme": "./README.md"
    }
  }